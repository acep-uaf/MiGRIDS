<!-- This file describes the units, scale and offset used for each data type used in the model. Datatypes are  -->


<unitDefaults>
    <!-- Real power -->
    <P units = 'kW' scale="1" offset="0" datatype = 'int32' />

    <!-- Reactive power -->
    <Q units = 'kvar' scale="1" offset="0" datatype = 'int32' />

    <!-- Apparent power -->
    <S units = 'kVA' scale="1" offset="0" datatype = 'int32' />

    <!-- wind speed-->
    <WS units="m/s" scale="1" offset="0" datatype="int32"/>

    <!-- Storage temperature-->
    <Tstorage units="K" scale="1000000" offset="0" datatype="int32"/>

    <!-- Ambient temperature-->
    <Tambient units="K" scale="1" offset="0" datatype="int32"/>

    <!-- time -->
    <time units="seconds since January 01 1970 (UTC)" scale="1" offset="0" datatype="int32"/>

    <!-- fuel flow -->
    <FF units="kg/s" scale="10000" offset="0" datatype="int32"/>
</unitDefaults>